{
  "name": "react-redux-javascript-boilerplate",
  "version": "0.0.1",
  "description": "Application boilerplate for React and Redux, implemented in Javascript.",
  "main": "src/index.html",
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint --syntax scss './src/**/*.scss' --ignore-path .gitignore",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier:check": "prettier --check '**/*.+(js|jsx|json|css|md|html)' --ignore-path .gitignore",
    "prettier:write": "prettier --write '**/*.+(js|jsx|json|css|md|html)' --ignore-path .gitignore",
   "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "author": {
    "name": "Artsiom Platkouski",
    "email": "komukc.apt@gmail.com",
    "url": "https://www.linkedin.com/in/artsiom-platkouski/"
  },
  "keywords": [
    "react",
    "redux",
    "javascript",
    "boilerplate"
  ],
  "homepage": "https://github.com/aplatkouski/react-redux-javascript-boilerplate#README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:aplatkouski/react-redux-javascript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/aplatkouski/react-redux-javascript-boilerplate/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "stylelint": "13.10.0",
    "stylelint-config-twbs-bootstrap": "2.1.0"
  }
}
